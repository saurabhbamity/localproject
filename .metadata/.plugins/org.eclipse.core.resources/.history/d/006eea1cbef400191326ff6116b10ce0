package com.test.java8.streamApi;

import java.util.ArrayList;
import java.util.List;

public class ParallelStreamExample {
	
	public static void main(String[] args) {
		
		List<> list= new ArrayList<>();
		list.
		
	}

}


class Student{
	
	private int id;
	private int age;
	private String name;
	private String Ddepartment;
	
	

	public Student(int id, int age, String name, String ddepartment) {
		super();
		this.id = id;
		this.age = age;
		this.name = name;
		Ddepartment = ddepartment;
	}
	
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDdepartment() {
		return Ddepartment;
	}

	public void setDdepartment(String ddepartment) {
		Ddepartment = ddepartment;
	}
	
}