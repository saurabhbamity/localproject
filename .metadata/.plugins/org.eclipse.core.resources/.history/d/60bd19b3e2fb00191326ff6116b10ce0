package com.test.concurrency.filereader;

import java.io.File;
import java.io.FileReader;
import java.io.IOException;

public class FileRead {

	public static void main(String[] args) {
		
		File f= new File("abc.txt");
		try {
		FileReader fr= new FileReader(f);
		}catch(IOException e) {
			e.printStackTrace();
		}
		
	}
	
	
}
